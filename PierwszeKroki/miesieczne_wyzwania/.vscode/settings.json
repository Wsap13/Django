{
    "python.pythonPath": "/usr/local/bin/python3",
    "python.languageServer": "Pylance",
    "window.zoomLevel": 1,
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Extension",
            "type": "extensionHost",
            "request": "launch",
            "runtimeExecutable": "${execPath}",
            "args": [
                "--extensionDevelopmentPath=${workspaceFolder}"
            ]
        }
    ],
}